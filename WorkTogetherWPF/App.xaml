<Application x:Class="WorkTogetherWPF.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:WorkTogetherWPF" xmlns:convertor="clr-namespace:WorkTogetherWPF.Convertor"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="./Styles/Style.xaml"/>
            </ResourceDictionary.MergedDictionaries>

            <convertor:ObjectToisReadOnlyConverter x:Key="SharedObjectToisReadOnlyConverter"/>
            <convertor:UserRoleToVisibilityConverter x:Key="SharedUserRoleToVisibilityConverter"/>
        </ResourceDictionary>
         
    </Application.Resources>
</Application>
